{
    "compilerOptions": {
    "jsx": "react-jsx",
        "module": "commonjs",
        "target": "es6",
        "allowJs": true,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
},
    "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"]
}
